{
    "name": "@ackee/chris",
    "version": "0.2.1",
    "description": "Useful additions for your redux - react-router based app",
    "main": "lib/index.js",
    "module": "es/index.js",
    "scripts": {
        "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
        "build:types": "tsc --project tsconfig.types.json --emitDeclarationOnly",
        "build": "npm run clean && npm run type-check && npm run build:js && npm run build:types",
        "storybook": "rm -rf build && start-storybook -p 6006",
        "build-storybook": "cross-env BABEL_ENV=development build-storybook -o build",
        "lint": "tslint \"src/**/*.{ts,tsx}\"",
        "test": "BABEL_ENV=test jest",
        "clean": "rm -rf lib",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check -- --watch",
        "start": "npm run build && onchange 'src/**/*.{ts,tsx}' -- npm run build"
    },
    "author": "Lukáš Horák <lukas.horak@ackee.cz>",
    "contributors": [
        "Jiří Čermák <jiri.cermak@ackee.cz>"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/AckeeCZ/chris"
    },
    "dependencies": {
        "@types/lodash": "^4.14.119",
        "@types/react-router": "^4.4.3",
        "lodash": "^4.17.11",
        "path-to-regexp": "^2.4.0",
        "utility-types": "^2.1.0"
    },
    "peerDependencies": {
        "prop-types": "^15.6.2",
        "react": "^16.6.3",
        "react-router": "^4.3.1",
        "redux-saga": "^0.16.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.0",
        "@babel/core": "^7.2.0",
        "@babel/plugin-proposal-class-properties": "^7.2.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
        "@babel/preset-env": "^7.2.0",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.1.0",
        "@storybook/addon-actions": "^4.1.3",
        "@storybook/addon-info": "^4.1.3",
        "@storybook/addon-links": "^4.1.3",
        "@storybook/addon-options": "^4.1.3",
        "@storybook/addon-storysource": "^4.1.3",
        "@storybook/react": "^4.1.3",
        "@types/react": "^16.7.13",
        "@types/react-dom": "^16.0.11",
        "@types/react-intl": "^2.3.14",
        "@types/react-redux": "^6.0.11",
        "@types/redux": "^3.6.0",
        "@types/storybook__react": "^4.0.0",
        "babel-jest": "^23.6.0",
        "babel-loader": "^8.0.4",
        "connected-react-router": "^6.0.0",
        "cross-env": "^5.2.0",
        "enzyme": "^3.8.0",
        "enzyme-adapter-react-16": "^1.7.1",
        "eslint": "^5.10.0",
        "eslint-config-ackee": "^0.9.0",
        "eslint-plugin-jest": "^22.1.2",
        "jest": "^23.6.0",
        "node-sass": "^4.11.0",
        "onchange": "^5.2.0",
        "prettier-config-ackee": "0.0.5",
        "prop-types": "^15.6.2",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-router": "^4.3.1",
        "redux-saga": "^0.16.2",
        "sass-loader": "^7.1.0",
        "tslint": "^5.12.0",
        "tslint-config-ackee": "^0.2.1",
        "tslint-config-airbnb": "^5.11.1",
        "tslint-config-prettier": "^1.17.0",
        "tslint-react": "^3.6.0",
        "typescript": "^3.2.2"
    },
    "jest": {
        "testPathIgnorePatterns": [
            "/node_modules",
            "/lib"
        ],
        "setupTestFrameworkScriptFile": "./configure-jest.js"
    },
    "publishConfig": {
        "access": "public"
    }
}
